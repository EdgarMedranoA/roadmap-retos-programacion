func main() {
    
    var autoKia: String = "Kia rio" // Variable
    let autoFord: String = "Ford mustang" // Constante (su valor no podra ser modificado)
    var lenguaje: String = "Swift"
    var cilindros: Int = 4
    var precioKia: Double = 150000.20
    var usado: Bool = true
    var precioFord: Float = 250000.20
    
    print("Â¡Hola \(lenguaje)!")
    print("imprimo resto de variables \(autoKia). \(cilindros). \(precioKia). \(precioFord). \(usado).")
}
